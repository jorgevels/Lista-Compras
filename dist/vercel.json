 {
    "version": 2,
    "name": "Mercando",
    "builds": [
        { "use": "@vercel/static-build", "dist": "package.json"}
    ],
    "routes": [
        {
      "dist": "(.*).css",
      "dest": "/$1.css"
    },
    {
      "dist": "(.*).js",
      "dest": "/$2.js"
    },
    {
      "dist": "(.*).png",
      "dest": "/$1.png"
    },
    {
      "dist": "(.*).svg",
      "dest": "/$1.svg"
    },
    {
      "dist": "(.*).ico",
      "dest": "/$1.ico"
    },
    {
      "dist": "(.*).json",
      "dest": "/$1.json"
    },
        {
            "dist": "/.*",
            "dest": "index.html"
        }
    ]
} 